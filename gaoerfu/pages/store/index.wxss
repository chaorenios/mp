/* pages/store/index.wxss */

.store-window {
  margin: 5px;
  position: relative;
}

.store-image-left {
  width: 46%;
  border-color: #f0f0f0;
  border-style: solid;
  border-width: 1px; 
  position: relative;
  left: 5px;
  top: 5px;
}

.store-image-right {
  width: 46%;
  border-color: #f0f0f0;
  border-style: solid;
  border-width: 1px; 
  position: absolute;
  right: 5px;
  top: 5px;
}